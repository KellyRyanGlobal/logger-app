pipeline {
    agent none
    environment {
        DOCKER_USER = credentials('docker-username')
        DOCKER_PASS = credentials('docker-password')
        APP_NAME = 'test_app'
        VERSION = '1.0'
    }
    stages {
        stage('Docker Build Test_App') {
            agent { dockerfile true }
            steps {
                sh 'uname -a'
                sh 'docker'
            }
        }
    }

}